<div class="tripAnalysis">
    <h1 class="center" style="color: coral;">Trip Analysis</h1>
</div>
<div class="container">
    <div class="row">
        <div id="listSubmissions">

        </div>

    </div>
    <div class="row">
        <!-- google map embed -->
        <div class="col s12 center">
            <iframe id="mapurl" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
    </div>
    <div class="row">
        <div class="col s12 m6 l6">
            <h4>From: <span id="from"></span></h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>To: <span id="to"></span></h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>Time: <span id="time"></span> Mins</h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>Distance: <span id="distance"></span> KM</h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>Average fuel consumption rate: <span id="aFuel"></span> L/100km</h4>
        </div>

        <div class="col s12 m6 l6">
            <h4>Average Speed: <span id="aSpeed"></span> km/h</h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>Average brake performance: <span id="aBrake"></span> %</h4>
        </div>
        <div class="col s12 m6 l6">
            <h4>Aveage Engine Temperature: <span id="aTemp"></span> â„ƒ</h4>
        </div>
    </div>

</div>